{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "singleQuote": true,
  "semi": false,
  "quoteProps": "preserve",
  "importOrder": [
    "^react$",
    "^next$",
    "^next/(.*)$",
    "^@radix-ui/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@/(.*)$",
    "^[./](.*)$"
  ],
  "importOrderSeparation": false
}
